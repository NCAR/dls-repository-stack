<metadataFieldInfo xmlns="http://www.dlese.org/Metadata/fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/fields http://www.dlese.org/Metadata/fields/annotation/1.0.00/pathway.xsd">
	<field name="Pathway" language="en-us" metaFormat="dlese_anno" metaVersion="1.0.00" path="/annotationRecord/service/pathway">
		<definition>the name of a group that enters resources directly into the DLESE Reviewed Collection (DRC)</definition>

		<vocabLevels number="1">
			<levelAndDeftn level="1" levelName="Pathway" levelDeftn="the name of a group that enters resources directly into the DLESE Reviewed Collection (DRC)"/>
		</vocabLevels>

		<terms>
			<termAndDeftn vocab="CRS (Community Review System)">self-explanatory</termAndDeftn>
			<termAndDeftn vocab="DWEL (Digital Water Education Library)">self-explanatory</termAndDeftn>
			<termAndDeftn vocab="ESEEREV (NASA Earth Science Enterprise Education Review System)">self-explanatory</termAndDeftn>
			<termAndDeftn vocab="JESSE (Journal of Earth System Science Education)">self-explanatory</termAndDeftn>
			<termAndDeftn vocab="MY NASA DATA (Mentoring and inquirY using NASA Data on Atmospheric and earth science for Teachers and Amateurs)">self-explanatory</termAndDeftn>
		</terms>


		<bestPractices>
			<dos>
				<practice status="current" originDate="2005-04-15">Use this field if making annotations for the DLESE Reviewed Collection (DRC). Such pathways must be approved by the DLESE Collections Accessioning Taskforce (email support@dlese.org).</practice>
			</dos>
			<donts>
				<practice status="current" originDate="2005-04-15">Do not use this field if making annotations that do not pertain to the DLESE Reviewed Collection.</practice>
			</donts>
		</bestPractices>

		<documentation>
			<obligation>optional</obligation>
			<min>0</min>
			<max>1</max>
			<XMLdataTypes>
				<XMLdataType>string</XMLdataType>
			</XMLdataTypes>
			<domain>controlled vocabulary</domain>
			<example>Community Review System (CRS)</example>
		</documentation>
	</field>
</metadataFieldInfo>
