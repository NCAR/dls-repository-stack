<opml xmlns="http://www.dlese.org/Metadata/opml" xmlns:groups="http://www.dlese.org/Metadata/ui/groups" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/opml http://www.dlese.org/Metadata/ui/groups/1.0/groups-opml.xsd">
<head>
<title>Role</title>
<groups:concept language="en-us" metaFormat="dlese_anno" metaVersion="1.0.00" text="Role" audience="default" path="/annotationRecord/annotation/contributors/contributor/@role" deftn="the role of a person or organization in making the annotation"/>
</head>
<body>
<outline groups:vocab="Author" type="vocab" text="Author" groups:deftn="a person or organization who creates content"/>
<outline groups:vocab="College educator" type="vocab" text="College educator" groups:deftn="a person with teaching responsibilities at the university level"/>
<outline groups:vocab="Contact" type="vocab" text="Contact" groups:deftn="a person or organization who can provide more information"/>
<outline groups:vocab="Contributor" type="vocab" text="Contributor" groups:deftn="a person or organization who provides content or information but not as the primary author"/>
<outline groups:vocab="Educator" type="vocab" text="Educator" groups:deftn="a person or organization with primary teaching responsibilities"/>
<outline groups:vocab="Elementary educator" type="vocab" text="Elementary educator" groups:deftn="a person with teaching responsibilities at the elementary level"/>
<outline groups:vocab="Evaluator" type="vocab" text="Evaluator" groups:deftn="a person or organization with primary evaluation responsibilities"/>
<outline groups:vocab="High school educator" type="vocab" text="High school educator" groups:deftn="a person with teaching responsibilities at the secondary education level"/>
<outline groups:vocab="Librarian" type="vocab" text="Librarian" groups:deftn="a person with expertise library management"/>
<outline groups:vocab="Middle school educator" type="vocab" text="Middle school educator" groups:deftn="a person with teaching responsibilities at the middle school level"/>
<outline groups:vocab="Publisher" type="vocab" text="Publisher" groups:deftn="an organization who makes content accessible"/>
<outline groups:vocab="Scientist" type="vocab" text="Scientist" groups:deftn="a person with expertise in a specific area of science"/>
<outline groups:vocab="Student" type="vocab" text="Student" groups:deftn="a student at any educational level"/>
</body>
</opml>
