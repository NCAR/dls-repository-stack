<opml xmlns="http://www.dlese.org/Metadata/opml" xmlns:groups="http://www.dlese.org/Metadata/ui/groups" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/opml http://www.dlese.org/Metadata/ui/groups/1.0/groups-opml.xsd">
<head>
<title>Share</title>
<groups:concept language="en-us" metaFormat="dlese_anno" metaVersion="1.0.00" text="Share" audience="default" path="/annotationRecord/annotation/contributors/contributor/@share" deftn="a boolean value that indicates whether a contributor's name, email address and organization is shared in systems and services"/>
</head>
<body>
<outline groups:vocab="true" type="vocab" text="true" groups:deftn="yes, the name, email address and organization of the contributor can be shared or exposed in systems and services"/>
<outline groups:vocab="false" type="vocab" text="false" groups:deftn="no, the name, email address and organization of the contributor cannot be shared or exposed in systems and services"/>
</body>
</opml>
