<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.00">
	<xsd:annotation>
		<xsd:documentation>*****  Overview  *****</xsd:documentation>
		<xsd:documentation>Purpose - controlled vocabulary schema for DLESE metadata frameworks</xsd:documentation>
		<xsd:documentation>Framework - annotation metadata framework</xsd:documentation>
		<xsd:documentation>Source - DLESE Program Center (DPC); http://www.dlese.org</xsd:documentation>
		<xsd:documentation>DLESE - Digital Library for Earth System Education</xsd:documentation>
	</xsd:annotation>

	<xsd:annotation>
		<xsd:documentation>***** History of change *****</xsd:documentation>
		<xsd:documentation>2006-02-16: Added: One star, Two star, Three star, Four star, Five star</xsd:documentation>
		<xsd:documentation>2006-02-16: Removed: Not recommended, Cautiously recommended, Recommended, Highly recommended</xsd:documentation>
		<xsd:documentation>2005-10-14: Added: Not recommended, Cautiously recommended, Recommended, Highly recommended</xsd:documentation>
		<xsd:documentation>2005-10-14: Removed: Poor, Fair, Good, Very good and Excellent</xsd:documentation>
		<xsd:documentation>2005-09-30: Created this controlled vocabulary schema file</xsd:documentation>
	</xsd:annotation>

	<xsd:annotation>
		<xsd:documentation>*****  Simple Types (alpha order) *****</xsd:documentation>
	</xsd:annotation>

	<xsd:simpleType name="ratingType">
		<xsd:annotation>
			<xsd:documentation>*****  ratingType  *****</xsd:documentation>
			<xsd:documentation>ratingType - a collection defined judgment of a resource where One star is always lower/worse than Five star</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="One star"/>
			<xsd:enumeration value="Two star"/>
			<xsd:enumeration value="Three star"/>
			<xsd:enumeration value="Four star"/>
			<xsd:enumeration value="Five star"/>
		</xsd:restriction>
	</xsd:simpleType>
