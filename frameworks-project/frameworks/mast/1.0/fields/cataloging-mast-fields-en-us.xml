<metadataFieldInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.dlsciences.org/frameworks/fields" xsi:schemaLocation="http://www.dlsciences.org/frameworks/fields http://www.dlsciences.org/frameworks/mast/1.0/schemas/fields/non-vocab-fields.xsd">
  <field name="Cataloging" metaFormat="mast" metaVersion="1.0" language="en-us" path="/record/contributions/cataloging/cataloging">
		<definition attribution="GEM 2.0 Element Descriptions at http://www.thegateway.org/about/documentation/metadataElements">Information about the agency that created the catalog record.</definition> 
		
		<bestPractices>
			<dos>
		
				<practice status="current" originDate="2008-10-20">Information about either the individual cataloger or the cataloging agency or both may be entered.</practice> <!--source:GEM-->
				<practice status="current" originDate="2008-10-20">For individuals, include first and last name, and affiliation if known.</practice> <!--source:UCAR/DLS-->

		</dos>
		</bestPractices>
  </field>
</metadataFieldInfo>
