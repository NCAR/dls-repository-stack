	<head>
		<title>DOI</title>
		
	<!--2011 OS submission form version when the form was used for cataloging
		<title>ID number type</title>
-->
		<concept language="en-us" metaFormat="osm" metaVersion="1.1" text="DOI" audience="submission" path="/record/classify/idNumber/@type" deftn="the Digital Object Identifier (DOI) of the item"/>
	</head>
	<body>
		<outline type="vocab" vocab="arXiv" text="arXiv" display="false"/>
		<outline type="vocab" vocab="CALL" text="Call" deftn="a unique identifier used to identify a specific copy and relative location of an item within a library's collection" display="false"/>
		<outline type="vocab" vocab="Citation/Article" text="Citation/Article" deftn="a unique reference label to ensure the persistent and actionable identification of resources" display="false"/>
		<outline type="vocab" vocab="DOI" text="DOI" deftn="Digital Object Identifier: a set of unique code used by publishers and other disseminators of digital content to ensure the persistent and actionable identification and interoperable exchange of that content"/>
		<outline type="vocab" vocab="EPRINTS" text="EPRINTS" deftn="a unique identifier used to ensure the persistent and actionable identification of resources held within a digital repository" display="false"/>
		<outline type="vocab" vocab="ISBN" text="ISBN" deftn="International Standard Book Number: a unique, standardized 10-digit number assigned to a specific edition of a book or other monographic publication" display="false"/>
		<outline type="vocab" vocab="ISSN" text="ISSN" deftn="International Standard Serial Number: a unique, standardized 8-digit number assigned to a specific serial title" display="false"/>
		<outline type="vocab" vocab="REPORT" text="REPORT" deftn="identifier used to reference a published report; particularly those published by government entities and other agencies" display="false"/>
		<outline type="vocab" vocab="SRef-ID" text="SRef-ID" display="false"/>
		<outline type="vocab" vocab="WMO" text="WMO" deftn="report identification numbers assigned by the World Meteorological Organization" display="false"/>
		<outline type="vocab" vocab="WOS" text="WOS" display="false"/>
	</body>
