	<head>
		<title>Funder's Name</title>
		<concept language="en-us" metaFormat="osm" metaVersion="1.1" text="Funder's Name" audience="submission" path="/record/classify/fundingEntity" deftn="The name of the funding agency or organization that contributed fiscally to the creation of the resource."/>
	</head>
	<body>
		<outline type="vocab" vocab="IBM Shared University Research Program" text="IBM Shared University Research Program"/>
		<outline type="vocab" vocab="National Aeronautics and Space Administration (NASA)" text="National Aeronautics and Space Administration (NASA)"/>
		<outline type="vocab" vocab="National Oceanic and Atmospheric Administration (NOAA)" text="National Oceanic and Atmospheric Administration (NOAA)"/>
		<outline type="vocab" vocab="National Science Foundation (NSF)" text="National Science Foundation (NSF)"/>
		<outline type="vocab" vocab="Office of Naval Research (ONR)" text="Office of Naval Research (ONR)"/>
		<outline type="vocab" vocab="U.S. Department of Defense (DOD)" text="U.S. Department of Defense (DOD)"/>
		<outline type="vocab" vocab="U.S. Department of Education" text="U.S. Department of Education"/>
		<outline type="vocab" vocab="U.S. Department of Energy (DOE)" text="U.S. Department of Energy (DOE)"/>
