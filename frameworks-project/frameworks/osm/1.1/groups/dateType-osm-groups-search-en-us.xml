		<outline type="vocab" vocab="Accepted" text="Accepted" display="false" deftn="date on which a work was selected for publication"/>
		<outline type="vocab" vocab="In press" text="In press" deftn="a date on which a work was in the process of being published"/>
		<outline type="vocab" vocab="Modified" text="Modified" display="false" deftn="date on which a resource was altered from a previous state"/>
		<outline type="vocab" vocab="Submitted" text="Submitted" display="false" deftn="date on which a work was sent to a publisher for consideration"/>		
