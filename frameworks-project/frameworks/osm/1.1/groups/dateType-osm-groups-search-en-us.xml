<opml xmlns="http://www.dlsciences.org/frameworks/fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.dlsciences.org/frameworks/fields http://nldr.library.ucar.edu/metadata/osm/1.1/schemas/fields/dateType.xsd"
version="2.0">
	<head>
		<title>Date type</title>
		<concept language="en-us" metaFormat="osm" metaVersion="1.1" text="Date Type" audience="search" path="/record/coverage/date/@type" deftn="the kind of date being associated with the resource"/>
	</head>
	<body>
		<outline type="vocab" vocab="Circa" text="Circa" deftn="approximate date on which a resource was produced; mainly for archival materials"/>
		<outline type="vocab" vocab="Created" text="Created" deftn="date on which a resource was produced; mainly for unpublished and archival materials"/>
		<outline type="vocab" vocab="Digitized" text="Digitized" display="false" deftn="date on which an analog or physical resource was transferred into or replicated by an electronic format"/>
		<outline type="vocab" vocab="Folder" text="Folder" display="false" deftn="date applied to a file unit of like materials"/>
		<outline type="vocab" vocab="Presented" text="Presented" deftn="date on which a work was communicated to an audience"/>
		<outline type="vocab" vocab="Published" text="Published" deftn="date on which a resource formally appeared in print"/>
</body>

</opml>
