			<xs:documentation>***** fundingEntityType  *****</xs:documentation>
			<xs:documentation>Valid values for the element</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="IBM Shared University Research Program"/>
			<xs:enumeration value="National Aeronautics and Space Administration (NASA)"/>
			<xs:enumeration value="National Oceanic and Atmospheric Administration (NOAA)"/>
