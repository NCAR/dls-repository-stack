<metadataFieldInfo xmlns="http://www.dlese.org/Metadata/fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/fields http://www.dlese.org/Metadata/fields/news-opps/1.0.00/non-vocab-fields.xsd">
	<field name="Source" language="en-us" metaFormat="news_opps" metaVersion="1.0.00" path="/news-oppsRecord/contributors/contributor/source">
		<definition>the RSS channel from which the news item came</definition>

		<bestPractices>
			<dos>
				<practice status="current" originDate="2004-01-20">Use this field only if describing contributions from an RSS channel.</practice>
			</dos>
		</bestPractices>

		<documentation>
			<obligation>choice</obligation>
			<min>1 or 0</min>
			<max>1</max>
			<attributes>
				<attribute>url</attribute>
			</attributes>
			<XMLdataTypes>
				<XMLdataType>complex</XMLdataType>
			</XMLdataTypes>
			<domain>free text</domain>
			<example>USGS Earthquakes</example>
		</documentation>
	</field>
</metadataFieldInfo>
