<opml xmlns="http://www.dlese.org/Metadata/opml" xmlns:groups="http://www.dlese.org/Metadata/ui/groups" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/opml http://www.dlese.org/Metadata/ui/groups/1.0/groups-opml.xsd">
<head>
<title>Role</title>
<groups:concept language="en-us" metaFormat="news_opps" metaVersion="1.0.00" text="Role" audience="default" path="/news-oppsRecord/contributors/contributor/@role" deftn="the function of a person or organization in regards to creating, providing, supporting, hosting or cataloging the news, event or opportunity"/>
</head>
<body>
<outline groups:vocab="Author" type="vocab" text="Author" groups:deftn="a person or organization who creates content"/>
<outline groups:vocab="Cataloger" type="vocab" text="Cataloger" groups:deftn="a person or organization who catalogs"/>
<outline groups:vocab="Contact" type="vocab" text="Contact" groups:deftn="a person or organization who can provide more information"/>
<outline groups:vocab="Editor" type="vocab" text="Editor" groups:deftn="a person or organization who performs quality control on metadata records"/>
<outline groups:vocab="Newsfeed" type="vocab" text="Newsfeed" groups:deftn="the RSS channel making the content accessible"/>
<outline groups:vocab="Publisher" type="vocab" text="Publisher" groups:deftn="a person or organization who makes content accessible"/>
<outline groups:vocab="Sponsor" type="vocab" text="Sponsor" groups:deftn="a person or organization who hosts or supports news, events or opportunities"/>
<outline groups:vocab="Validator" type="vocab" text="Validator" groups:deftn="a person or organization who accessions metadata records into a collection"/>
</body>
</opml>
