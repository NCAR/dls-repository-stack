	<xsd:annotation>
		<xsd:documentation>*****  	DLESEgradeRangeType  *************************</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="DLESEgradeRangeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DLESE:Primary elementary"/>
			<xsd:enumeration value="DLESE:Intermediate elementary"/>
			<xsd:enumeration value="DLESE:Middle school"/>
			<xsd:enumeration value="DLESE:High school"/>
			<xsd:enumeration value="DLESE:Undergraduate lower division"/>
			<xsd:enumeration value="DLESE:Undergraduate upper division"/>
			<xsd:enumeration value="DLESE:Graduate or professional"/>
			<xsd:enumeration value="DLESE:Informal education"/>
			<xsd:enumeration value="DLESE:General public"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:annotation>
		<xsd:documentation>*****  DLESEgradeRangeLeafType  *************************</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="DLESEgradeRangeLeafType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Primary elementary"/>
			<xsd:enumeration value="Intermediate elementary"/>
