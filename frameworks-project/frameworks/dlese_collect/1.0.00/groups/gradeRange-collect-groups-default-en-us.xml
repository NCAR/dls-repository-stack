<opml xmlns="http://www.dlese.org/Metadata/opml" xmlns:groups="http://www.dlese.org/Metadata/ui/groups" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/opml http://www.dlese.org/Metadata/ui/groups/1.0/groups-opml.xsd">
<head>
<title>Grade range</title>
<groups:concept id="gr" language="en-us" metaFormat="dlese_collect" metaVersion="1.0.00" text="Grade range" audience="default" path="/collectionRecord/general/gradeRanges/gradeRange" deftn="the appropriate grade range or context for users of items in the collection"/>
</head>
<body>
<outline groups:vocab="DLESE:General public" type="vocab" groups:id="00" text="General public" groups:deftn="learners in the general population"/>
<outline groups:vocab="DLESE:Graduate or professional" type="vocab" groups:id="01" text="Graduate or professional" groups:deftn="learners who have completed an undergraduate degree or who advancing their own knowledge as it relates to their employment"/>
<outline groups:vocab="DLESE:High school" type="vocab" groups:id="02" text="High school" groups:deftn="learners in grades 9-12"/>
<outline groups:vocab="DLESE:Informal education" type="vocab" groups:id="03" text="Informal education" groups:deftn="learners in museum settings, community continuing education programs, community technology centers, park naturalists settings, nature centers and home schooling"/>
<outline groups:vocab="DLESE:Intermediate elementary" type="vocab" groups:id="04" text="Intermediate elementary" groups:deftn="learners in grades 3-5"/>
<outline groups:vocab="DLESE:Middle school" type="vocab" groups:id="05" text="Middle school" groups:deftn="learners in grades 6-8"/>
<outline groups:vocab="DLESE:Primary elementary" type="vocab" groups:id="07" text="Primary elementary" groups:deftn="learners in grades K-2"/>
<outline groups:vocab="DLESE:Undergraduate lower division" type="vocab" groups:id="09" text="Undergraduate lower division" groups:deftn="generally learners in the first two years of a baccalaureate degree or completing an introductory course"/>
<outline groups:vocab="DLESE:Undergraduate upper division" type="vocab" groups:id="0a" text="Undergraduate upper division" groups:deftn="generally learners in the last two years of a baccalaureate degree or completing an advanced course"/>
<outline groups:vocab="DLESE:Not applicable" type="vocab" groups:id="06" text="Not applicable" groups:deftn="it is not appropriate to assign a grade range to the collection"/>
<outline groups:vocab="DLESE:To be supplied" type="vocab" groups:id="08" text="To be supplied" groups:deftn="grade range information is currently unknown but will be supplied at a later date"/>
</body>
