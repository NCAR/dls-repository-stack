<opml xmlns="http://www.dlese.org/Metadata/opml" xmlns:groups="http://www.dlese.org/Metadata/ui/groups" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/opml http://www.dlese.org/Metadata/ui/groups/1.0/groups-opml.xsd">
<head>
<title>Detailed geometry - type</title>
<groups:concept language="en-us" metaFormat="adn" metaVersion="0.7.00" text="Detailed geometry - type" audience="default" path="/itemRecord/geospatialCoverages/geospatialCoverage/detGeos/detGeo/typeDetGeo" deftn="the type of geographic footprint"/>
</head>
<body>
<outline groups:vocab="Bounding box" type="vocab" text="Bounding box" groups:deftn="a regular polygon, defined by 4 longitude-latitude pairs, that is parallel to the equator that encloses the area of interest"/>
<outline groups:vocab="Point" type="vocab" text="Point" groups:deftn="a single longitude-latitude pair"/>
<outline groups:vocab="Polygon" type="vocab" text="Polygon" groups:deftn="a closed polygon that is defined with at least 3 longitude-latitude pairs and where the beginning and ending points are the same"/>
<outline groups:vocab="Polyline" type="vocab" text="Polyline" groups:deftn="a string of at least 3 longitude-latitude pairs where the beginning and ending points are not the same"/>
</body>
</opml>
