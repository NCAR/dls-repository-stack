			<xsd:enumeration value="Teacher educators"/>
			<xsd:enumeration value="Teachers"/>
			<xsd:enumeration value="Teaching professionals"/>
			<xsd:enumeration value="Technology coordinators"/>
		</xsd:restriction>
	</xsd:simpleType>
