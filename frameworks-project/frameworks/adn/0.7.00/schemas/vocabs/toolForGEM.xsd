<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.7.00">
	<xsd:annotation>
		<xsd:documentation>
		**********************************************************************  Overview  **************************************************************
			Definition: 			Controlled vocabulary schema for DLESE metadata frameworks
			Framework use: 	ADN-I (item)
			Source org:			Gateway to Educational Materials (GEM); http://www.geminfo.org/
			Vocab values:		http://geminfo.org/Workbench/Metadata/Vocab_Audience.html
			Vocab last update:	1999-09-23
			DPC last update:		2003-01-16
			Notes:				GEM vocab adapted to meet DLESE needs (terms, capitalization, punctuation)
		</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation/>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>***********************  Simple Types (alpha order) *************************************</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="GEMtoolForType">
		<xsd:annotation>
			<xsd:documentation>
				*************************************** GEMtoolForType  *****************************************
				Lists the values that will appear in the metadata record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="GEM:Administrators"/>
			<xsd:enumeration value="GEM:University instructors"/>
			<xsd:enumeration value="GEM:Curriculum supervisors"/>
			<xsd:enumeration value="GEM:Elementary school teachers"/>
			<xsd:enumeration value="GEM:General public"/>
			<xsd:enumeration value="GEM:Librarians"/>
			<xsd:enumeration value="GEM:Middle school teachers"/>
			<xsd:enumeration value="GEM:Other educational professionals"/>
			<xsd:enumeration value="GEM:Parents"/>
			<xsd:enumeration value="GEM:Policymakers"/>
			<xsd:enumeration value="GEM:Secondary school teachers"/>
			<xsd:enumeration value="GEM:Student teachers"/>
			<xsd:enumeration value="GEM:Students"/>
			<xsd:enumeration value="GEM:Supervisors"/>
			<xsd:enumeration value="GEM:Teacher educators"/>
			<xsd:enumeration value="GEM:Teachers"/>
			<xsd:enumeration value="GEM:Teaching professionals"/>
			<xsd:enumeration value="GEM:Technology coordinators"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="GEMtoolForLeafType">
		<xsd:annotation>
			<xsd:documentation>
				*************************************** GEMtoolForLeafType  *****************************************
				Lists leaf values
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Administrators"/>
			<xsd:enumeration value="University instructors"/>
