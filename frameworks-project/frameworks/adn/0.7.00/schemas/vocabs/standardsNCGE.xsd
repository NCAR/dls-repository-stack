	<xsd:annotation>
		<xsd:documentation>***********************  Simple Types (alpha order) *************************************</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="NCGEgeographyContentStandardsType">
		<xsd:annotation>
			<xsd:documentation>
				******************************  NCGEgeographyContentStandardsType  *********************************
				Lists the values that will appear in the metadata record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NCGE:The World in Spatial Terms:How to use maps and other geographic representations, tools, and technologies to acquire, process, and report information"/>
			<xsd:enumeration value="NCGE:The World in Spatial Terms:How to use mental maps to organize information about people, places, and environments"/>
			<xsd:enumeration value="NCGE:The World in Spatial Terms:How to analyze the spatial organization of people, places, and environments on Earth's surface"/>
			<xsd:enumeration value="NCGE:Places and Regions:The physical and human characteristics of places"/>
			<xsd:enumeration value="NCGE:Places and Regions:That people create regions to interpret Earth's complexity"/>
			<xsd:enumeration value="NCGE:Places and Regions:How culture and experience influence people's perception of places and regions"/>
			<xsd:enumeration value="NCGE:Physical Systems:The physical processes that shape the patterns of Earth's surface"/>
			<xsd:enumeration value="NCGE:Physical Systems:The characteristics and spatial distribution of ecosystems on Earth's surface"/>
			<xsd:enumeration value="NCGE:Human Systems:The characteristics, distribution, and migration of human populations on Earth's surface"/>
			<!--For the next benchmark, DLESE-IMS has 'mosaic' rather than 'mosaics' 			-->
			<xsd:enumeration value="NCGE:Human Systems:The characteristics, distributions, and complexity of Earth's cultural mosaics"/>
			<xsd:enumeration value="NCGE:Human Systems:The patterns and networks of economic interdependence on Earth's surface"/>
			<xsd:enumeration value="NCGE:Human Systems:The process, patterns, and functions of human settlement"/>
			<xsd:enumeration value="NCGE:Human Systems:How forces of cooperation and conflict among people influence the division and control of Earth's surface"/>
			<xsd:enumeration value="NCGE:Environment and Society:How human actions modify the physical environment"/>
			<xsd:enumeration value="NCGE:Environment and Society:How physical systems affect human systems"/>
			<xsd:enumeration value="NCGE:Environment and Society:The changes that occur in the meaning, use, distribution, and importance of resources"/>
			<xsd:enumeration value="NCGE:The Uses of Geography:How to apply geography to interpret the past"/>
			<xsd:enumeration value="NCGE:The Uses of Geography:How to apply geography to interpret the present and plan for the future"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NCGEgeographyContentStandardsLeafType">
		<xsd:annotation>
			<xsd:documentation>
				******************************  NCGEgeographyContentStandardsLeafType  *********************************
				Lists leaf values
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="How to use maps and other geographic representations, tools, and technologies to acquire, process, and report information"/>
			<xsd:enumeration value="How to use mental maps to organize information about people, places, and environments"/>
