	<xsd:simpleType name="DLESEkindType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  DLESEkindType  *****************************************
				Lists the values that will appear in the metadata record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DLESE:Has thumbnail"/>
			<xsd:enumeration value="DLESE:Is based on"/>
			<xsd:enumeration value="DLESE:Is basis for"/>
			<xsd:enumeration value="DLESE:Is associated with"/>
