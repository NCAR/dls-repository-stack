<metadataFieldInfo xmlns="http://www.dlese.org/Metadata/fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/fields http://www.dlese.org/Metadata/fields/adn-item/0.7.00/non-vocab-fields.xsd">
	<field name="Catalog entry - list - repository" language="en-us" metaFormat="adn" metaVersion="0.7.00" path="/itemRecord/metaMetadata/catalogEntries">
		<definition>a list of catalog information from the library/repository that directly describes the resource</definition>

		<bestPractices>
			<dos>
				<practice status="current" originDate="2006-02-27" link1="catalog entry - repository" style1="bold">Complete the required subfield of catalog entry - repository.</practice>
			</dos>
		</bestPractices>

		<documentation>
			<obligation>required</obligation>
			<children>
				<child>catalog</child>
			</children>
			<min>1</min>
			<max>1</max>
			<XMLdataTypes>
				<XMLdataType>complex</XMLdataType>
			</XMLdataTypes>
			<domain>the child element</domain>
			<example>&lt;catalog entry="MY-LIBRARY-000-000-000-123"&gt;My Library Collection&lt;/catalog&gt;</example>
			<other>true</other>
			<notes>
				<note>Other XPATH: /itemRecord/general/catalogEntries</note>
			</notes>
		</documentation>


	</field>
</metadataFieldInfo>
