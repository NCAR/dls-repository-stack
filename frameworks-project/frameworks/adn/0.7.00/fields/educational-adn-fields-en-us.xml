<metadataFieldInfo xmlns="http://www.dlese.org/Metadata/fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/fields http://www.dlese.org/Metadata/fields/adn-item/0.7.00/non-vocab-fields.xsd">
	<field name="Educational" language="en-us" metaFormat="adn" metaVersion="0.7.00" path="/itemRecord/educational">
		<definition>educational or pedagogic features of the resource including audience, resource type, standards</definition>

		<bestPractices>
			<dos>
				<practice status="current" originDate="2005-04-19" link1="audiences" style1="bold" link2="resource types" style2="bold" link3="grade range" style3="bold">Complete the required subfields of audiences and resources types. The audiences subfield contains the additional required subfield of grade range.</practice>
			</dos>
		</bestPractices>

		<documentation>
			<children>
				<child>audiences</child>
				<child>resourceTypes</child>
				<child>contentStandards</child>
				<child>teachingStandards</child>
				<child>processStandards</child>
				<child>interactivityLevel</child>
				<child>interactvityType</child>
				<child>description</child>
			</children>
			<obligation>required</obligation>
			<min>1</min>
			<max>1</max>
			<XMLdataTypes>
				<XMLdataType>complex</XMLdataType>
			</XMLdataTypes>
			<domain>the child elements</domain>
			<example>&lt;audiences&gt;, &lt;resourceTypes&gt;</example>
		</documentation>


	</field>
</metadataFieldInfo>
