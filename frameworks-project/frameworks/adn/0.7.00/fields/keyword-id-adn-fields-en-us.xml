<metadataFieldInfo xmlns="http://www.dlese.org/Metadata/fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/fields http://www.dlese.org/Metadata/fields/adn-item/0.7.00/non-vocab-fields.xsd">
	<field name="Source id - keyword" language="en-us" metaFormat="adn" metaVersion="0.7.00" path="/itemRecord/general/keywords/keyword/@id">
		<definition>a unique identification number from the source cited for the keyword</definition>

		<bestPractices>
			<dos>
				<practice status="current" originDate="2006-02-27">Enter keyword id information when the keyword is derived from an external library/repository or thesaurus and it is desirable to access the source of the term.</practice>
			</dos>
			<examples>
				<practice status="current" originDate="2006-09-20">Id: SoMeLIB-123</practice>
				<practice status="current" originDate="2006-09-20">Src: The ABC Thesaurus</practice>
				<practice status="current" originDate="2006-09-20">Url: http://some.url.org</practice>
			</examples>
		</bestPractices>

		<documentation>
			<obligation>optional</obligation>
			<min>0</min>
			<max>1</max>
			<XMLdataTypes>
				<XMLdataType>string</XMLdataType>
			</XMLdataTypes>
			<domain>free text</domain>
			<example>SoMeLIB-123</example>
		</documentation>


	</field>
</metadataFieldInfo>
