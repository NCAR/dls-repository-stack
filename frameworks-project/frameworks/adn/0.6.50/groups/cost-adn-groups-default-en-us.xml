<opml xmlns="http://www.dlese.org/Metadata/opml" xmlns:groups="http://www.dlese.org/Metadata/ui/groups" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/opml http://www.dlese.org/Metadata/ui/groups/1.0/groups-opml.xsd">
<head>
<title>Cost</title>
<groups:concept language="en-us" metaFormat="adn" metaVersion="0.6.50" text="Cost" audience="default" path="/itemRecord/rights/cost" deftn="an indication of whether use or access of the resource requires payment"/>
</head>
<body>
<outline groups:vocab="DLESE:No" type="vocab" text="No" groups:deftn="there is no cost or a very nominal cost associated with using or accessing the resource"/>
<outline groups:vocab="DLESE:To be supplied" type="vocab" text="To be supplied" groups:deftn="not immediately known, therefore will be supplied at a future date"/>
<outline groups:vocab="DLESE:Unknown" type="vocab" text="Unknown" groups:deftn="there may or may not be a cost associated with using or accessing the resource, that is, the cost could not be determined"/>
<outline groups:vocab="DLESE:Yes" type="vocab" text="Yes" groups:deftn="there is a cost (more than nominal) associated with using or accessing the resource"/>
</body>
</opml>
