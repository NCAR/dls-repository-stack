<opml xmlns="http://www.dlese.org/Metadata/opml" xmlns:groups="http://www.dlese.org/Metadata/ui/groups" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/opml http://www.dlese.org/Metadata/ui/groups/1.0/groups-opml.xsd">
<head>
<title>Interactivity - type</title>
<groups:concept language="en-us" metaFormat="adn" metaVersion="0.6.50" text="Interactivity - type" audience="default" path="/itemRecord/educational/interactivityType" deftn="the type of interactions supported by a resource"/>
</head>
<body>
<outline groups:vocab="LOM:Active" type="vocab" text="Active" groups:deftn="information flows from the resource to the learner and from the learner to the resource"/>
<outline groups:vocab="LOM:Expositive" type="vocab" text="Expositive" groups:deftn="information flows mainly from the resource to the learner; describes information"/>
<outline groups:vocab="LOM:Mixed" type="vocab" text="Mixed" groups:deftn="a combination of Active and Expositive such that neither one dominates"/>
<outline groups:vocab="LOM:Undefined" type="vocab" text="Undefined" groups:deftn="the type of interactions supported by the resource can not be determined"/>
</body>
</opml>
