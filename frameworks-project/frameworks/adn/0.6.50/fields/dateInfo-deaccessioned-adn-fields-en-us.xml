<metadataFieldInfo xmlns="http://www.dlese.org/Metadata/fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.dlese.org/Metadata/fields http://www.dlese.org/Metadata/fields/adn-item/0.6.50/non-vocab-fields.xsd">
	<field name="Date deaccessioned" language="en-us" metaFormat="adn" metaVersion="0.6.50" path="/itemRecord/metaMetadata/dateInfo/@deaccessioned">
		<definition>the deaccession date of the metadata record out of a repository</definition>

		<bestPractices>
			<dos>
				<practice status="current" originDate="2004-09-03">Enter a date to signify the deaccessioning of the metadata record.</practice>
				<practice status="current" originDate="2004-09-03">Use a date format of YYYY-MM-DD, e.g. 2004-12-27.</practice>
				<practice status="current" originDate="2004-09-03">Enter a year for every date entered.</practice>
				<practice status="current" originDate="2005-01-31">Catalogers should always include a date.</practice>
			</dos>

			<donts>
				<practice status="current" originDate="2004-09-03">Do not enter days of the week.</practice>
				<practice status="current" originDate="2004-09-03">Do not enter just months or days.</practice>
				<practice status="current" originDate="2006-02-28">Do not enter a time.</practice>
			</donts>

			<examples>
				<practice status="current" originDate="2004-10-15">Year: 2004.</practice>
				<practice status="current" originDate="2004-10-15">Year and month: 2004-12.</practice>
				<practice status="current" originDate="2004-10-15">Year, month and day: 2004-12-27.</practice>
			</examples>

		</bestPractices>


		<documentation>
			<obligation>optional</obligation>
			<min>0</min>
			<max>1</max>
			<XMLdataTypes>
				<XMLdataType>date</XMLdataType>
				<XMLdataType>gYearMonth</XMLdataType>
				<XMLdataType>gYear</XMLdataType>
			</XMLdataTypes>
			<domain>numerical date format</domain>
			<example>2004-10-15, 2004-10,  2004</example>
		</documentation>
	</field>
</metadataFieldInfo>
