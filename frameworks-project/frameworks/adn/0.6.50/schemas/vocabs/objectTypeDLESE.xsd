<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.6.50">
	<xsd:annotation>
		<xsd:documentation>
		**********************************************************************  Schema Overview  **************************************************************
			Definition: 			Controlled vocabulary schema for DLESE metadata frameworks
			Framework use: 		ADN-I (item)
			Source org:			DLESE Program Center (DPC); http://www.dlese.org
			Source values:		DPC
			Source last update:	2003-02-13
			DPC last update:		2002-02-13
			Notes: 				Developed and maintained by the DPC.		
		</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>***************** History of Change ************************
2003-02-13: Change vocab term to be not available.
2003-02-13: Possible terms were: Bok, Rock, Thermometer, Dirt, Ice core
		</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>***********************  Simple Types (alpha order) *************************************</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="DLESEobjectTypeType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  DLESEobjectTypeType  *****************************************
				Lists the values that will appear in the metadata record
				Unknown is not part of the vocabulary because this field is not required.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DLESE:No vocabulary available yet:Do not use"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DLESEobjectTypeLeafType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  DLESEobjectTypeLeafType  *****************************************
				Lists leaf values
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Do not use"/>
		</xsd:restriction>
	</xsd:simpleType>
