	<xsd:annotation>
		<xsd:documentation>
		**********************************************************************  Overview  **************************************************************
			Definition: 			Controlled vocabulary schema for DLESE metadata frameworks
			Framework use: 		ADN-I (item)
			Source org:			DLESE Program Center (DPC); http://www.dlese.org
			Source values:		DPC
			Source last update:	2002-10-31
			DPC last update:		2002-10-31
			Notes: 				Developed and maintained by the DPC.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>************************  Complex Types  ******************************************</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>***********************  Simple Types  *************************************</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="missingType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  missingType  *****************************************
				Lists the values that will appear in the metadata record.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DLESE:Missing"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="missingLeafType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  missingLeafType  *********************************************
			Leaf values.
		</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Missing"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="unknownType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  unknownType  *****************************************
				Lists the values that will appear in the metadata record.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DLESE:Unknown"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="unknownLeafType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  unknownLeafType  *********************************************
			Leaf values.
		</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Unknown"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="notapplicableType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  notapplicableType  *****************************************
				Lists the values that will appear in the metadata record.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DLESE:Not applicable"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="notapplicableLeafType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  notapplicableLeafType  *********************************************
			Leaf values.
		</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Not applicable"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="suppliedType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  suppliedType  *****************************************
				Lists the values that will appear in the metadata record.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DLESE:To be supplied"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="suppliedLeafType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  suppliedLeafType  *********************************************
			Leaf values.
		</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="To be supplied"/>
		</xsd:restriction>
	</xsd:simpleType>
