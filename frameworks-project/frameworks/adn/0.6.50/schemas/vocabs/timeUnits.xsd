<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.6.50">
	<xsd:annotation>
		<xsd:documentation>
		**********************************************************************  Schema Overview  **************************************************************
			Definition: 			Controlled vocabulary schema for DLESE metadata frameworks
			Framework use: 	ADN-I (item)
			Source org:			DLESE Program Center (DPC); http://www.dlese.org
			Vocab values:		DPC
			Vocab last update:	2003-01-16
			DPC last update:		2003-01-16
			Notes: 				Developed and maintained at the DPC.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>***********************  Simple Types (alpha order) *************************************</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="timeUnitsType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  timeUnitsType  *****************************************
				Lists the values that will appear in the metadata record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Minutes ago"/>
			<xsd:enumeration value="Hours ago"/>
			<xsd:enumeration value="Days ago"/>
			<xsd:enumeration value="Years ago"/>
			<xsd:enumeration value="Hundred years ago"/>
			<xsd:enumeration value="Thousand years ago"/>
			<xsd:enumeration value="Million years ago"/>
			<xsd:enumeration value="Billion years ago"/>
			<xsd:enumeration value="Minutes forward"/>
			<xsd:enumeration value="Hours forward"/>
			<xsd:enumeration value="Days forward"/>
			<xsd:enumeration value="Years forward"/>
			<xsd:enumeration value="Hundred years forward"/>
			<xsd:enumeration value="Thousand years forward"/>
			<xsd:enumeration value="Million years forward"/>
			<xsd:enumeration value="Billion years forward"/>
		</xsd:restriction>
	</xsd:simpleType>
