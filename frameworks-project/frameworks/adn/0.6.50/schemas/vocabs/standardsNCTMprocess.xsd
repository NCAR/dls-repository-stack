	<xsd:simpleType name="NCTMmathProcessStandardsType">
		<xsd:annotation>
			<xsd:documentation>
				***************************************  NCTMmathProcessStandardsType  ***************************************
				Lists the values that will appear in the metadata record
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NCTM:Problem Solving:Build new mathematical knowledge through problem solving"/>
			<xsd:enumeration value="NCTM:Problem Solving:Solve problems that arise in mathematics and in other contexts"/>
			<xsd:enumeration value="NCTM:Problem Solving:Apply and adapt a variety of appropriate strategies to solve problems"/>
			<xsd:enumeration value="NCTM:Problem Solving:Monitor and reflect on the process of mathematical problem solving"/>
			<xsd:enumeration value="NCTM:Reasoning and Proof:Recognize reasoning and proof as fundamental aspects of mathematics"/>
			<xsd:enumeration value="NCTM:Reasoning and Proof:Make and investigate mathematical conjectures"/>
			<xsd:enumeration value="NCTM:Reasoning and Proof:Develop and evaluate mathematical arguments and proofs"/>
			<xsd:enumeration value="NCTM:Reasoning and Proof:Select and use various types of reasoning and methods of proof"/>
			<xsd:enumeration value="NCTM:Communication:Organize and consolidate their mathematical thinking through communication"/>
			<xsd:enumeration value="NCTM:Communication:Communicate their mathematical thinking coherently and clearly to peers, teachers, and others"/>
			<xsd:enumeration value="NCTM:Communication:Analyze and evaluate the mathematical thinking and strategies of others"/>
			<xsd:enumeration value="NCTM:Communication:Use the language of mathematics to express mathematical ideas precisely"/>
			<xsd:enumeration value="NCTM:Connections:Recognize and use connections among mathematical ideas"/>
			<xsd:enumeration value="NCTM:Connections:Understand how mathematical ideas interconnect and build on one another to produce a coherent whole"/>
			<xsd:enumeration value="NCTM:Connections:Recognize and apply mathematics in contexts outside of mathematics"/>
			<xsd:enumeration value="NCTM:Representation:Create and use representations to organize, record, and communicate mathematical ideas"/>
			<xsd:enumeration value="NCTM:Representation:Select, apply, and translate among mathematical representations to solve problems"/>
			<xsd:enumeration value="NCTM:Representation:Use representations to model and interpret physical, social, and mathematical phenomena"/>
		</xsd:restriction>
	</xsd:simpleType>
