<?xml version="1.0" encoding="UTF-8"?>
<termsRecord xmlns="http://www.vocabs.dlese" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.vocabs.dlese
http://www.dlese.org/Metadata/vocabularies/management/adn-i/cost.xsd">
	<field name="cost" id="co" language="en-us">
		<definition system="meta" interface="default">Whether use or access of the resource requires payment</definition>
		<definition system="dcs" interface="default">Whether use or access of the resource requires payment</definition>
		<definition system="dds" interface="default">String</definition>

		<value vocab="DLESE:No" id="00">
			<definition system="meta" interface="default">Means there is no cost associated with using or accessing the resource</definition>
			<definition system="dcs" interface="default">Means there is no cost associated with using or accessing the resource</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="DLESE:Unknown" id="01">
			<definition system="meta" interface="default">Means there may or may not be a cost associated with using or accessing the resource, that is the cost could not be determined</definition>
			<definition system="dcs" interface="default">Means there may or may not be a cost associated with using or accessing the resource, that is the cost could not be determined</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="DLESE:Yes" id="02">
			<definition system="meta" interface="default">Means there is a cost associated with using or accessing the resource</definition>
			<definition system="dcs" interface="default">Means there is a cost associated with using or accessing the resource</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="DLESE:To be supplied" id="03">
			<definition system="meta" interface="default">Not immediately known, therefore will be supplied at a future date</definition>
			<definition system="dcs" interface="default">Not immediately known, therefore will be supplied at a future date</definition>
			<definition system="dds" interface="default"></definition>
		</value>

	</field>
</termsRecord>
