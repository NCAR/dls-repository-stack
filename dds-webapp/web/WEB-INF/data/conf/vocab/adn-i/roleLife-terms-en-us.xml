<?xml version="1.0" encoding="UTF-8"?>
<termsRecord xmlns="http://www.vocabs.dlese" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.vocabs.dlese
http://www.dlese.org/Metadata/vocabularies/management/adn-i/roleLife.xsd">
	<field name="roleLife" id="rl" language="en-us">
		<definition system="meta" interface="default">The role of persons or organizations contributing to the content of a resource, includes author, contact and publisher information</definition>
		<definition system="dds" interface="default">String</definition>

		<value vocab="Author" id="">
			<definition system="meta" interface="default">The person(s) or entity responsible for creating intellectual content of resources</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="Contact" id="">
			<definition system="meta" interface="default">The person(s) or entity to contact regarding use, copyright, or availability of a resource</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="Contributor" id="">
			<definition system="meta" interface="default">Any person(s) or entity who helps in creating the intellectual content or design of a resource</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="Editor" id="">
			<definition system="meta" interface="default">The person(s) or entity who edits the content of a resource</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="Principal Investigator" id="">
			<definition system="meta" interface="default">The person(s) listed on a National Science Foundation (NSF) grant under which a resource is created</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

		<value vocab="Publisher" id="">
			<definition system="meta" interface="default">The person(s) or entity responsible for making a resource available</definition>
			<definition system="dds" interface="default">String</definition>
		</value>

	</field>
</termsRecord>
