<?xml version="1.0" encoding="UTF-8" ?>

<frameworkConfigRecord xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ns.nsdl.org/ncs/
framework_config/1.00/schemas/dcsFrameworkConfig.xsd">
  <xmlFormat>osm_collect</xmlFormat>
  <schemaInfo>
    <rootElementName>collectionRecord</rootElementName>
    <schemaURI>http://nldr.library.ucar.edu/metadata/osm_collect/1.0/schemas/collection.xsd</schemaURI>
    <paths>
      <path pathName="id" readOnly="true" requiredByCopyRecord="true" 
                requiredByMinimalRecord="true">/collectionRecord/recordID</path>
      <path pathName="recordDate" readOnly="true" requiredByCopyRecord="true" 
                requiredByMinimalRecord="true" valueType="date">/collectionRecord/recordDate</path>
    </paths>
  </schemaInfo>
  <editorInfo>
    <renderer>DleseEditorRenderer</renderer>
    <baseRenderLevel>2</baseRenderLevel>
    <rebuildOnStart>true</rebuildOnStart>


  </editorInfo>
  <name>osm_collect</name>
</frameworkConfigRecord>
