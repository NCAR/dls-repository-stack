<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Jonathan Ostwald (UCAR) --><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="frameworkConfigRecord" type="frameworkConfigType">		<xsd:annotation>			<xsd:documentation>schema for Metadata Framework configuration files used by the DCS</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:complexType name="frameworkConfigType">		<xsd:all>			<xsd:element name="name" type="xsd:string" minOccurs="0">				<xsd:annotation>					<xsd:documentation>optional display name for the framework</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="xmlFormat" type="xsd:string">				<xsd:annotation>					<xsd:documentation>required format key for the framework (e.g., adn, news_opps)</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="schemaInfo" type="schemaInfoType">				<xsd:annotation>					<xsd:documentation>information to aid in working with the schema for this framework, including the URI to the schema itself</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="editorInfo" type="editorInfoType">				<xsd:annotation>					<xsd:documentation>informationto aid and configure the metadata editor.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="standAloneInfo" type="standAloneInfoType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>information to configure a metadata editor as "stand alone" rather than as integrated with the DCS repository</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:all>	</xsd:complexType>	<xsd:complexType name="schemaInfoType">		<xsd:all>			<xsd:element name="rootElementName" type="xsd:string"/>			<xsd:element name="nameSpaceInfo" type="xsd:string"/>			<xsd:element name="schemaURI" type="xsd:string"/>			<xsd:element name="version" type="xsd:string" minOccurs="0"/>			<xsd:element name="paths" type="pathsType" minOccurs="0"/>		</xsd:all>	</xsd:complexType>	<xsd:complexType name="editorInfoType">		<xsd:all>			<xsd:element name="renderer" type="rendererType"/>			<xsd:element name="baseRenderLevel" type="xsd:integer"/>			<xsd:element name="firstPage" type="xsd:string"/>			<xsd:element name="editorConfig" type="editorConfigType"/>			<xsd:element name="rebuildOnStart" type="xsd:boolean"/>			<xsd:element name="discussionURL" type="stringTextType" minOccurs="0"/>			<xsd:element name="editorPages" type="editorPagesType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>if the editor requires multiple pages (as in the case with large frameworks) this element specifies the pages and provides a human readable label for each</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="mdvFields" type="mdvFieldsType" minOccurs="0">				<xsd:annotation>					<xsd:documentation>specifies fields that are rendered from information from the MetaDataVocab manager. </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="fieldInfoURI" type="stringTextType" minOccurs="0"/>		</xsd:all>	</xsd:complexType>	<xsd:simpleType name="rendererType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="SimpleJspRenderer"/>			<xsd:enumeration value="BasicJspRenderer"/>			<xsd:enumeration value="CollapsibleJspRenderer"/>			<xsd:enumeration value="DleseJspRenderer"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="editorConfigType">		<xsd:annotation>			<xsd:documentation source="configures the metadata editor for this framework. noframes is the default."/>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:enumeration value="frames"/>			<xsd:enumeration value="noframes"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:complexType name="mdvFieldsType">		<xsd:sequence>			<xsd:element name="mdvField" type="mdvFieldType" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="mdvFieldType">		<xsd:simpleContent>			<xsd:extension base="stringTextType">				<xsd:attribute name="fieldName" type="xsd:string" use="required"/>			</xsd:extension>		</xsd:simpleContent>	</xsd:complexType>	<xsd:complexType name="editorPagesType">		<xsd:sequence>			<xsd:element name="editorPage" type="editorPageType" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="editorPageType">		<xsd:simpleContent>			<xsd:extension base="stringTextType">				<xsd:attribute name="pageLabel" type="xsd:string" use="optional"/>			</xsd:extension>		</xsd:simpleContent>	</xsd:complexType>	<xsd:complexType name="fieldInfoType">		<xsd:sequence>			<xsd:element name="fieldInfoURI" type="stringTextType" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:simpleType name="stringTextType">		<xsd:annotation>			<xsd:documentation>*****  stringTextType  *************************</xsd:documentation>			<xsd:documentation>Use this patterned string to ensure that a null tag is not valid string content. Note: that xsd:string allows content to be null even if the element is required.</xsd:documentation>		</xsd:annotation>		<xsd:restriction base="xsd:string">			<xsd:pattern value=".[\S\s]*"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:complexType name="standAloneInfoType">		<xsd:all>			<xsd:element name="sampleRecordPath" type="xsd:string" minOccurs="0"/>			<xsd:element name="recordsDir" type="xsd:string"/>		</xsd:all>	</xsd:complexType>	<xsd:complexType name="pathsType">		<xsd:annotation>			<xsd:documentation> a way to define framework-specific paths</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="path" maxOccurs="unbounded">				<xsd:complexType>					<xsd:simpleContent>						<xsd:extension base="pathType">							<xsd:attribute name="pathName" use="required"/>						</xsd:extension>					</xsd:simpleContent>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="pathType">		<xsd:simpleContent>			<xsd:extension base="stringTextType">				<xsd:attribute name="pathName" use="required"/>				<xsd:attribute name="defaultValue" use="optional"/>				<xsd:attribute name="valueType" use="optional" default="String"/>				<xsd:attribute name="requiredByMinimalRecord" type="xsd:boolean" use="optional" default="false"/>				<xsd:attribute name="requiredByCopyRecord" type="xsd:boolean" use="optional" default="false"/>				<xsd:attribute name="readOnly" type="xsd:boolean" use="optional"/>			</xsd:extension>		</xsd:simpleContent>	</xsd:complexType></xsd:schema>